# MedVoice Environment Variables
# Copy this file to .env and fill in your API keys

# Required - Deepgram API key for STT/TTS
# Get your key at: https://console.deepgram.com/
DEEPGRAM_API_KEY=your_deepgram_api_key_here

# Required - Google Gemini API key for LLM
# Get your key at: https://makersuite.google.com/app/apikey
GEMINI_API_KEY=your_gemini_api_key_here

# Optional - Daily.co API key for WebRTC voice transport
# Get your key at: https://dashboard.daily.co/developers
DAILY_API_KEY=your_daily_api_key_here

# Optional settings
DEBUG=false
LOG_LEVEL=INFO

# CORS origins (comma-separated)
CORS_ORIGINS=["http://localhost:5173","http://localhost:3000"]
